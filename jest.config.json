{
  "verbose": true,
  "clearMocks": false,
  "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.[jt]sx?$",
  "setupFilesAfterEnv": [
    "<rootDir>/setupTests.js"
  ],
  "collectCoverageFrom": [
    "src/components/**/*.(js|jsx)",
    "src/common.js",
    "src/reducers/**/*.js"
  ],
  "testPathIgnorePatterns": [
    "<rootDir>/node_modules/",
    "<rootDir>/build/",
    "<rootDir>/stories/",
    "<rootDir>/.storybook/",
    "<rootDir>/storybook-static/"
  ]
}